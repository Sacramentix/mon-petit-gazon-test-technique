{
  "name": "mpg",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "check:type": "tsc --noEmit",
    "check:env": "node --loader ts-node/esm --experimental-specifier-resolution=node src/env/index.ts",
    "couchbase:docker": "docker run -d --name db -p 8091-8097:8091-8097 -p 9123:9123 -p 11207:11207 -p 11210:11210 -p 11280:11280 -p 18091-18097:18091-18097 couchbase",
    "couchbase:init": "node --loader ts-node/esm --experimental-specifier-resolution=node test/couchbase/loadSampleData.ts",
    "dev": "node --watch --loader ts-node/esm --experimental-specifier-resolution=node src/index.ts",
    "build": "tsc",
    "start": "node dist"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "couchbase": "^4.2.4",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "zod": "^3.21.4"
  },
  "engines": {
    "node": ">=18.11.0"
  }
}
